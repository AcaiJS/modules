{
	"name": "@acai/router",
	"version": "2.2.0-alpha2",
	"description": "An agnostic router that is part of the Açaí Framework ecosytem that can be easily extended to be used both on the backend and frontend.",
	"repository": "https://gitlab.com/acaijs/router.git",
	"author": "Rafael <rafael.chaves98@hotmail.com>",
	"keywords": [
		"router",
		"acaijs",
		"module",
		"typescript",
		"route"
	],
	"license": "BSD-3-Clause",
	"private": false,
	"module": "dist/index.esm.js",
	"main": "dist/index.cjs.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/index.esm.js",
			"require": "./dist/index.cjs.js"
		}
	},
	"scripts": {
		"build": "node ../m_runtime/dist/cli.js && tsc --declaration --emitDeclarationOnly",
		"test": "ts-node ./test.ts --path \"./src/**/*.{test,tests}.{js,ts}\"",
		"lint": "eslint --quiet **/*",
		"lint:fix": "eslint --quiet **/* --fix",
		"prepublishOnly": "yarn build"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"devDependencies": {
		"@acai/preset": "0.2.0-alpha",
		"@types/node": "^14.14.28",
		"eslint": "^7.31.0",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.1.5"
	},
	"peerDependencies": {
		"ts-node-dev": "^1.1.8"
	},
	"dependencies": {
		"@acai/testing": "3.0.0-alpha"
	}
}
