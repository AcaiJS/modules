{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "strict": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "outDir": "./dist",
    "sourceMap": true,
    "declaration": true,
    "declarationDir": "./dist",
    "strictNullChecks": false,
    "skipLibCheck": true
  },
  "include": [
	"./**/*"
  ],
  "exclude": [
	  "./dist/**/*",
	  "./node_modules/**/*",
	  "./**/*.test.*"
	]
}
