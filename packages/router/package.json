	},
	"devDependencies": {
		"@acai/testing": "^3.0.0-beta.0",
		"@types/node": "^14.14.28",
		"eslint": "~8.4.0",
		"typescript": "^4.1.5",
		"@typescript-eslint/eslint-plugin": "~5.5.0",
		"@typescript-eslint/parser": "~5.5.0",
