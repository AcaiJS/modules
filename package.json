{
	"name": "acai",
	"description": "The monorepo responsible for handling all of açaí's modules into a centralized space",
	"author": "APO Softworks",
	"license": "BSD-3-Clause",
	"repository": {
		"type": "git",
		"url": "https://github.com/AcaiJS/modules"
	},
	"private": true,
	"workspaces": {
		"packages": [
			"packages/*"
		]
	},
	"devDependencies": {
		"eslint": "^7.31.0",
		"husky": "^7.0.0",
		"syncpack": "^5.7.11"
	},
	"scripts": {
		"aconfig": "cd ./packages/config && yarn",
		"interfaces": "cd ./packages/interfaces && yarn",
		"model": "cd ./packages/model && yarn",
		"preset": "cd ./packages/preset && yarn",
		"query": "cd ./packages/query && yarn",
		"router": "cd ./packages/router && yarn",
		"server": "cd ./packages/server && yarn",
		"stubber": "cd ./packages/stubber && yarn",
		"testing": "cd ./packages/testing && yarn",
		"validator": "cd ./packages/validator && yarn",
		"prepare": "husky install",
		"lint": "yarn eslint"
	}
}