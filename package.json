{
	"name": "acai",
	"description": "The monorepo responsible for handling all of açaí's modules into a centralized space",
	"author": "APO Softworks",
	"license": "BSD-3-Clause",
	"repository": {
		"type": "git",
		"url": "https://github.com/AcaiJS/modules"
	},
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"devDependencies": {
		"eslint": "^7.31.0",
		"lerna": "^4.0.0",
		"lint-staged": "^11.0.1",
		"syncpack": "^5.7.11",
		"husky": "^7.0.0"
	},
	"scripts": {
		"pre-commit": "lint-staged",
		"all:publish": "yarn lerna publish from-git --yes",
		"all:test": "yarn lerna run test --stream",
		"all:version": "yarn lerna version",
		"config:build": "yarn workspace @acai/config build",
		"config:prepublishOnly": "yarn workspace @acai/config prepublishOnly",
		"config:test": "yarn workspace @acai/config test",
		"delete:node_modules": "find . -name \"node_modules\" -exec rm -rf '{}' +",
		"format": "eslint --fix --ext .js,jsx,.ts,.tsx .",
		"interfaces:build": "yarn workspace @acai/interfaces build",
		"interfaces:lint:fix": "yarn workspace @acai/interfaces lint:fix",
		"interfaces:lint": "yarn workspace @acai/interfaces lint",
		"interfaces:prepublishOnly": "yarn workspace @acai/interfaces prepublishOnly",
		"lerna": "./node_modules/.bin/lerna",
		"lint": "yarn eslint -c ./packages/preset/.eslintrc.js",
		"model:build": "yarn workspace @acai/model build",
		"model:prepublishOnly": "yarn workspace @acai/model prepublishOnly",
		"model:start": "yarn workspace @acai/model start",
		"model:test": "yarn workspace @acai/model test",
		"prepare": "husky install",
		"query:build": "yarn workspace @acai/query build",
		"query:lint:fix": "yarn workspace @acai/query lint:fix",
		"query:lint": "yarn workspace @acai/query lint",
		"query:prepublishOnly": "yarn workspace @acai/query prepublishOnly",
		"query:start": "yarn workspace @acai/query start",
		"query:test": "yarn workspace @acai/query test",
		"router:build": "yarn workspace @acai/router build",
		"router:lint:fix": "yarn workspace @acai/router lint:fix",
		"router:lint": "yarn workspace @acai/router lint",
		"router:prepublishOnly": "yarn workspace @acai/router prepublishOnly",
		"router:test": "yarn workspace @acai/router test",
		"server:build": "yarn workspace @acai/server build",
		"server:lint:fix": "yarn workspace @acai/server lint:fix",
		"server:lint": "yarn workspace @acai/server lint",
		"server:prepublishOnly": "yarn workspace @acai/server prepublishOnly",
		"server:start": "yarn workspace @acai/server start",
		"server:test": "yarn workspace @acai/server test",
		"stubber:build": "yarn workspace @acai/stubber build",
		"stubber:prepublishOnly": "yarn workspace @acai/stubber prepublishOnly",
		"stubber:start": "yarn workspace @acai/stubber start",
		"stubber:test": "yarn workspace @acai/stubber test",
		"testing:build": "yarn workspace @acai/testing build",
		"testing:prepublishOnly": "yarn workspace @acai/testing prepublishOnly",
		"testing:test": "yarn workspace @acai/testing test",
		"validator:build": "yarn workspace @acai/validator build",
		"validator:prepublishOnly": "yarn workspace @acai/validator prepublishOnly",
		"validator:test": "yarn workspace @acai/validator test"
	}
}
