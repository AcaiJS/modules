{
	"name": "acai",
	"description": "The monorepo responsible for handling all of açaí's modules into a centralized space",
	"author": "APO Softworks",
	"license": "BSD-3-Clause",
	"repository": {
		"type": "git",
		"url": "https://github.com/AcaiJS/modules"
	},
	"private": true,
	"workspaces": {
		"packages": [
			"packages/*"
		]
	},
	"devDependencies": {
		"eslint": "^7.31.0",
		"husky": "^7.0.0",
		"syncpack": "^5.7.11"
	},
	"scripts": {
		"config": "cd ./packages/config",
		"interfaces": "cd ./packages/interfaces",
		"model": "cd ./packages/model",
		"query": "cd ./packages/query",
		"router": "cd ./packages/router",
		"server": "cd ./packages/server",
		"stubber": "cd ./packages/stubber",
		"testing": "cd ./packages/testing",
		"validator": "cd ./packages/validator",
		"prepare": "husky install",
		"lint": "yarn eslint"
	}
}